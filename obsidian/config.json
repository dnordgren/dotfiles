{
  "enabledPlugins": [],
  "theme": "obsidian",
  "pluginEnabledStatus": {
    "file-explorer": true,
    "global-search": true,
    "switcher": true,
    "graph": true,
    "backlink": true,
    "command-palette": true,
    "markdown-importer": true,
    "word-count": true,
    "custom-css": true,
    "tag-pane": true,
    "page-preview": true,
    "random-note": true,
    "outline": true,
    "open-with-default-app": true,
    "workspaces": true
  },
  "foldHeading": true,
  "foldIndent": true,
  "readableLineLength": false,
  "promptDelete": false,
  "showLineNumber": true,
  "vimMode": true,
  "alwaysUpdateLinks": true,
  "newFileLocation": "folder",
  "newFileFolderPath": "Hudl-Product Stack/inbox",
  "attachmentFolderPath": "Hudl-Product Stack/assets",
  "hotkeys": {
    "workspace:split-vertical": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "V"
      }
    ],
    "workspace:split-horizontal": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "H"
      }
    ],
    "workspace:copy-url": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "u"
      }
    ],
    "app:go-back": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "ArrowLeft"
      }
    ],
    "app:go-forward": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "ArrowRight"
      }
    ],
    "app:toggle-left-sidebar": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "b"
      }
    ],
    "app:toggle-right-sidebar": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "d"
      }
    ],
    "editor:toggle-fold": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "c"
      }
    ],
    "editor:swap-line-up": [
      {
        "modifiers": [
          "Alt"
        ],
        "key": "ArrowUp"
      }
    ],
    "editor:swap-line-down": [
      {
        "modifiers": [
          "Alt"
        ],
        "key": "ArrowDown"
      }
    ],
    "editor:open-search-replace": [],
    "editor:toggle-highlight": [
      {
        "modifiers": [
          "Mod"
        ],
        "key": "h"
      }
    ],
    "editor:delete-paragraph": [],
    "explorer:reveal-active-file": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "r"
      }
    ],
    "switcher:open": [
      {
        "modifiers": [
          "Mod"
        ],
        "key": ","
      }
    ],
    "app:open-settings": [],
    "graph:open": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "g"
      }
    ],
    "graph:open-local": [
      {
        "modifiers": [
          "Mod"
        ],
        "key": "g"
      }
    ],
    "backlink:open-backlinks": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "b"
      }
    ],
    "command-palette:open": [
      {
        "modifiers": [
          "Mod",
          "Shift"
        ],
        "key": "p"
      }
    ],
    "outline:open": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "o"
      }
    ],
    "open-with-default-app:open": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "e"
      }
    ],
    "open-with-default-app:show": [
      {
        "modifiers": [
          "Ctrl",
          "Mod"
        ],
        "key": "f"
      }
    ]
  },
  "useTab": false,
  "lineWrap": true
}
